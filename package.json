{
  "name": "vttprime",
  "version": "0.1.8",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "(npm version patch --no-git-tag-version) & (node versionUpdate) & (vue-cli-service build)",
    "lint": "vue-cli-service lint",
    "deploy": "aws s3 sync ./dist s3://vttprime",
    "test": "(npm version patch --no-git-tag-version) & (node versionUpdate) & (type src\\appVer.js)"
  },
  "dependencies": {
    "@doodle3d/clipper-js": "^1.0.11",
    "@pixi/filter-drop-shadow": "^3.1.1",
    "core-js": "^3.6.4",
    "jquery": "^3.5.1",
    "pixi.js": "^5.2.3",
    "splitpanes": "^2.2.1",
    "v-hotkey": "^0.8.0",
    "vue": "^2.6.11",
    "vue-multipane": "^0.9.5",
    "vue-pixi": "^0.1.1",
    "vue-resize-split-pane": "^0.1.5",
    "vuedraggable": "^2.23.2",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "compression-webpack-plugin": "^4.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-constant-condition": "warn",
      "no-redeclare": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
